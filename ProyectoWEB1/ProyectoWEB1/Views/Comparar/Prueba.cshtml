@model IEnumerable<ProyectoWEB1.Models.ViewModels.MaterialesViewModel>

@{
    ViewBag.Title = "Prueba";
}

<h2>Prueba</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TipoConstruccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrecioTotal)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrecioTotal)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdArchivoProyecto }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdArchivoProyecto }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdArchivoProyecto })
        </td>
    </tr>
}

</table>
